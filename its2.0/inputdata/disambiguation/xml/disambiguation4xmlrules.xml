<its:rules xmlns:its="http://www.w3.org/2005/11/its" version="2.0">
    <its:param name="disambigCityParam">dublin</its:param>
    <its:param name="disambigCapitalParam">capital</its:param>
   <its:disambiguationRule selector="/text/body/p/*[@id='dublin']"
      disambigClassRefPointer="@classref"
      disambigGranularity="entity"
      disambigIdentRefPointer="@identref"/>
    <its:disambiguationRule selector="/text/body/p/*[@id='capital']"
      disambigClassRefPointer="@classref"
      disambigGranularity="lexical-concept"
      disambigSourcePointer="@source"
      disambigIdentPointer="@ident"/>
  </its:rules>
